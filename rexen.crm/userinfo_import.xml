<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <BatchSize>1000</BatchSize>
    <EntityName>Userinfo</EntityName>
    <FileName>userinfo_import.csv</FileName>
    <ExternalId>EmployeeNumber</ExternalId>
    <Database>crm_mysql</Database>
    <Max>100000</Max>
    <Operation>Merge</Operation>
    <!-- <Operation>Delete</Operation> -->
    <Fields>
        <ColumnName>EmployeeNumber</ColumnName>
        <DataType>String</DataType>
        <FieldName>EmployeeNumber</FieldName>
    </Fields>    
    <Fields>
        <ColumnName>登录名</ColumnName>
        <DataType>String</DataType>
        <FieldName>loginName</FieldName>
    </Fields>    
    <Fields>
        <ColumnName>Id</ColumnName>
        <DataType>String</DataType>
        <FieldName>Id</FieldName>
    </Fields>
    <Fields>
        <ColumnName>姓名</ColumnName>
        <DataType>String</DataType>
        <FieldName>name</FieldName>
    </Fields>
    <Fields>
        <ColumnName>email</ColumnName>
        <DataType>String</DataType>
        <FieldName>email</FieldName>
    </Fields>
    <Fields>
        <ColumnName>loginName</ColumnName>
        <DataType>String</DataType>
        <FieldName>loginName</FieldName>
    </Fields>
    <Fields>
        <ColumnName>Password</ColumnName>
        <DataType>Password</DataType>
        <FieldName>Password</FieldName>
    </Fields>
    <Fields>
        <ColumnName>有效无效</ColumnName>
        <DataType>Integer</DataType>
        <FieldName>pl1</FieldName>
    </Fields>
    <Fields>
        <ColumnName>是否激活</ColumnName>
        <DataType>Integer</DataType>
        <FieldName>isActivited</FieldName>
    </Fields>
    <Fields>
        <ColumnName>Ts</ColumnName>
        <DataType>AutoTimeMillis</DataType>
        <FieldName>Ts</FieldName>
    </Fields>
</Configuration>