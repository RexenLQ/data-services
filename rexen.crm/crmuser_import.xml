<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Configuration>
    <BatchSize>1000</BatchSize>
    <EntityName>Crmuser</EntityName>
    <ExternalId>Code</ExternalId>
    <FileName>crmuser_import.csv</FileName>
    <Database>crm_mysql</Database>
    <Max>100000</Max>
    <Fields>
        <ColumnName>岗位名称</ColumnName>
        <DataType>String</DataType>
        <FieldName>Name</FieldName>
    </Fields>
    <Fields>
        <ColumnName>岗位编码</ColumnName>
        <DataType>String</DataType>
        <FieldName>code</FieldName>
    </Fields>
    <Fields>
        <ColumnName>上级岗位</ColumnName>
        <DataType>Lookup</DataType>
        <FieldName>reportto</FieldName>
        <LookupEntityName>Crmuser</LookupEntityName>
        <LookupFieldName>Code</LookupFieldName>
        <TargetFieldName>Id</TargetFieldName>
    </Fields>
    <Fields>
        <ColumnName>角色</ColumnName>
        <DataType>Lookup</DataType>
        <FieldName>role</FieldName>
        <LookupEntityName>Role</LookupEntityName>
        <LookupFieldName>Val</LookupFieldName>
        <TargetFieldName>Id</TargetFieldName>
    </Fields>    
    <Fields>
        <ColumnName>有效无效</ColumnName>
        <DataType>Lookup</DataType>
        <FieldName>pl1</FieldName>
        <LookupEntityName>CrmuserPl1</LookupEntityName>
        <LookupFieldName>Val</LookupFieldName>
        <TargetFieldName>Id</TargetFieldName>
    </Fields>    
    <Fields>
        <ColumnName>产品线</ColumnName>
        <DataType>Lookup</DataType>
        <FieldName>pl2</FieldName>
        <LookupEntityName>CrmuserPl2</LookupEntityName>
        <LookupFieldName>Val</LookupFieldName>
        <TargetFieldName>Id</TargetFieldName>
    </Fields>    
 
    <Fields>
        <ColumnName>城市</ColumnName>
        <DataType>Lookup</DataType>
        <FieldName>city</FieldName>
        <LookupEntityName>City</LookupEntityName>
        <LookupFieldName>Val</LookupFieldName>
        <TargetFieldName>Id</TargetFieldName>
    </Fields>    

    <Fields>
        <ColumnName>一级区域</ColumnName>
        <DataType>Lookup</DataType>
        <FieldName>pl4</FieldName>
        <LookupEntityName>RegionLevel1Pl</LookupEntityName>
        <LookupFieldName>Val</LookupFieldName>
        <TargetFieldName>Id</TargetFieldName>
    </Fields>   
    <Fields>
        <ColumnName>二级区域</ColumnName>
        <DataType>Lookup</DataType>
        <FieldName>pl5</FieldName>
        <LookupEntityName>RegionLevel2Pl</LookupEntityName>
        <LookupFieldName>Val</LookupFieldName>
        <TargetFieldName>Id</TargetFieldName>
    </Fields>

</Configuration>